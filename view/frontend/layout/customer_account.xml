<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<update handle="customer_account"/>
<body>
<referenceBlock name="customer_account_navigation">
<block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-custom">
<arguments>
<argument name="path" xsi:type="string">customerimage/customer/index</argument>
<argument name="label" xsi:type="string">Update Profile Image</argument>
</arguments>
</block>

</referenceBlock> 

<referenceContainer name="customer.account.dashboard.info.blocks"> 
<block class="Mageget\CustomerImage\Block\Customer\CustomerDetails" name="my_tab_deepak" template="Mageget_CustomerImage::cinfo.phtml" cacheable="false" before="-"> </block> 
</referenceContainer> 


</body>
</page>